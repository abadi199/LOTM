--coins
function make_coins()
	coin_visible=true
	cx=36
	cy=36
end

function update_coins()
	if (coin_visible and 
		px+6>=cx and px+6<cx+8 and
		py+1>=cy and py+1<cy+8) then
		sfx(0)
		coin_visible=false
	end
end

function draw_coins()
	if (coin_visible) then
		spr(4,36,36)
	end
end
