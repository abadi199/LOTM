--player
function make_player()
	px=64
	py=64
	psprite=0
end

function move_player()
	if (btn(⬅️)) px-=1 --left
	if (btn(➡️)) px+=1 --right
	if (btn(⬆️)) py-=1 --up
	if (btn(⬇️)) py+=1 --down
	
end

function draw_player()
	spr(psprite,px,py,2,2)
end

function draw_map()
	map(0,0,0,0,64,64)
end


