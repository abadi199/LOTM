	player.flip=false
end

function move(center,left,right)
	if player.timer < 3 then 
		player.sprite=center
	elseif player.timer < 6 then 
		player.sprite=right
	elseif player.timer < 9 then 
		player.sprite=center
	elseif player.timer < 12 then
		player.sprite=left
	elseif player.timer < 15 then
		player.timer=1
	end
end

function idle()
	player.sprite=2
end

function draw_player()
	if player.spinning then
		spr_r(player.sprite,player.x,player.y,player.angle,2,2)
	else 
		spr(player.sprite,player.x,player.y,2,2,player.flip)
	end
	print(player.angle)
	print(player.timer)
end

